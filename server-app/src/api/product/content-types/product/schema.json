{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "text"
    },
    "price": {
      "type": "biginteger"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::name.name",
      "mappedBy": "products"
    },
    "product_brands": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-brand.product-brand",
      "inversedBy": "products"
    },
    "product_colors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-color.product-color",
      "inversedBy": "products"
    },
    "product_comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-comment.product-comment",
      "mappedBy": "product"
    }
  }
}
